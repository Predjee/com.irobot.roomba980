{
  "id": "com.irobot.roomba980",
  "name": {
    "en": "iRobot"
  },
  "tags": {
    "en": [
      "roomba",
      "vacuum cleaner",
      "vacuum",
      "irobot",
      "mob"
    ],
    "nl": [
      "roomba",
      "stofzuiger",
      "irobot",
      "dweil"
    ]
  },
  "category": "appliances",
  "sdk": 3,
  "version": "4.0.1",
  "compatibility": ">=5.0.0",
  "author": {
    "name": "Michel Falke",
    "email": "michel@yiggle.nl"
  },
  "contributors": {
    "developers": [
      {
        "name": "Athom B.V.",
        "email": "info@athom.com"
      }
    ]
  },
  "contributing": {
    "donate": {
      "paypal": {
        "username": "Predjee"
      }
    }
  },
  "bugs": {
    "url": "https://github.com/Predjee/com.irobot.roomba980/issues"
  },
  "source": "https://github.com/Predjee/com.irobot.roomba980",
  "support": "mailto:michel@yiggle.nl",
  "description": {
    "en": "iRobot support for Homey",
    "nl": "iRobot ondersteuning voor Homey"
  },
  "images": {
    "large": "/assets/images/large.jpg",
    "small": "/assets/images/small.jpg"
  },
  "flow": {
    "conditions": [
      {
        "id": "bin_full",
        "title": {
          "en": "Bin !{{is|isn't}} full",
          "nl": "Vuilbak !{{is|is niet}} vol"
        },
        "args": [
          {
            "name": "device",
            "type": "device",
            "filter": "driver_id=irobot|roomba980"
          }
        ]
      },
      {
        "id": "bin_present",
        "title": {
          "en": "Bin !{{is|isn't}} present",
          "nl": "Vuilbak !{{is|is niet}} aanwezig"
        },
        "args": [
          {
            "name": "device",
            "type": "device",
            "filter": "driver_id=irobot|roomba980"
          }
        ]
      },
      {
        "id": "tank_full",
        "title": {
          "en": "Tank !{{is|isn't}} full",
          "nl": "Tank !{{is|is niet}} vol"
        },
        "args": [
          {
            "type": "device",
            "name": "device",
            "filter": "driver_id=braava"
          }
        ]
      },
      {
        "id": "tank_present",
        "title": {
          "en": "Tank !{{is|isn't}} present",
          "nl": "Tank !{{is|is niet}} aanwezig"
        },
        "args": [
          {
            "type": "device",
            "name": "device",
            "filter": "driver_id=braava"
          }
        ]
      },
      {
        "id": "lid_closed",
        "title": {
          "en": "Lid !{{is|isn't}} closed",
          "nl": "Deksel !{{is|is niet}} dicht"
        },
        "args": [
          {
            "type": "device",
            "name": "device",
            "filter": "driver_id=braava"
          }
        ]
      },
      {
        "id": "no_pad_detected",
        "title": {
          "en": "There !{{is|isn't}} a pad detected",
          "nl": "Er !{{is een|is geen}} dweildoek gedetecteerd"
        },
        "args": [
          {
            "type": "device",
            "name": "device",
            "filter": "driver_id=braava"
          }
        ]
      }
    ]
  },
  "drivers": [
    {
      "name": {
        "en": "iRobot mob",
        "nl": "iRobot dweil"
      },
      "class": "vacuumcleaner",
      "energy": {
        "batteries": [
          "INTERNAL"
        ]
      },
      "capabilities": [
        "vacuumcleaner_state",
        "measure_battery",
        "tank_full",
        "tank_present",
        "lid_closed",
        "detected_pad"
      ],
      "images": {
        "large": "/drivers/braava/assets/images/large.png",
        "small": "/drivers/braava/assets/images/small.png"
      },
      "pair": [
        {
          "id": "list_devices",
          "template": "list_devices",
          "navigation": {
            "next": "add_braava"
          },
          "options": {
            "singular": true
          }
        },
        {
          "id": "add_braava"
        }
      ],
      "id": "braava"
    },
    {
      "name": {
        "en": "iRobot vacuumcleaner",
        "nl": "iRobot stofzuiger"
      },
      "class": "vacuumcleaner",
      "energy": {
        "batteries": [
          "INTERNAL"
        ]
      },
      "capabilities": [
        "vacuumcleaner_state",
        "measure_battery",
        "bin_full",
        "bin_present"
      ],
      "images": {
        "large": "/drivers/irobot/assets/images/large.png",
        "small": "/drivers/irobot/assets/images/small.png"
      },
      "pair": [
        {
          "id": "list_devices",
          "template": "list_devices",
          "navigation": {
            "next": "add_irobot"
          },
          "options": {
            "singular": true
          }
        },
        {
          "id": "add_irobot"
        }
      ],
      "id": "irobot"
    },
    {
      "name": {
        "en": "Roomba 980",
        "nl": "Roomba 980"
      },
      "deprecated": true,
      "class": "vacuumcleaner",
      "energy": {
        "batteries": [
          "INTERNAL"
        ]
      },
      "capabilities": [
        "vacuumcleaner_state",
        "measure_battery",
        "bin_full",
        "bin_present"
      ],
      "images": {
        "large": "/drivers/roomba980/assets/images/large.png",
        "small": "/drivers/roomba980/assets/images/small.png"
      },
      "pair": [
        {
          "id": "list_devices",
          "template": "list_devices",
          "navigation": {
            "next": "add_irobot"
          },
          "options": {
            "singular": true
          }
        },
        {
          "id": "add_irobot"
        }
      ],
      "id": "roomba980"
    }
  ],
  "capabilities": {
    "bin_full": {
      "title": {
        "en": "Bin full",
        "nl": "Afvalbak vol"
      },
      "type": "boolean",
      "getable": true,
      "setable": false,
      "uiComponent": "sensor",
      "icon": "/drivers/irobot/assets/bin.svg"
    },
    "bin_present": {
      "title": {
        "en": "Bin present",
        "nl": "Afvalbak aanwezig"
      },
      "type": "boolean",
      "getable": true,
      "setable": false,
      "uiComponent": "sensor",
      "icon": "/drivers/irobot/assets/bin.svg"
    },
    "detected_pad": {
      "title": {
        "en": "Detected pad",
        "nl": "Dweildoek aawezig"
      },
      "type": "boolean",
      "getable": true,
      "setable": false,
      "uiComponent": "sensor",
      "icon": "/drivers/braava/assets/pad.svg"
    },
    "lid_closed": {
      "title": {
        "en": "Lid is closed",
        "nl": "Deksel is dicht"
      },
      "type": "boolean",
      "getable": true,
      "setable": false,
      "uiComponent": "sensor",
      "icon": "/drivers/braava/assets/lid.svg"
    },
    "tank_full": {
      "title": {
        "en": "Watertank full",
        "nl": "Watertank vol"
      },
      "type": "boolean",
      "getable": true,
      "setable": false,
      "uiComponent": "sensor",
      "icon": "/drivers/braava/assets/tank_full.svg"
    },
    "tank_present": {
      "title": {
        "en": "Watertank present",
        "nl": "Watertank aanwezig"
      },
      "type": "boolean",
      "getable": true,
      "setable": false,
      "uiComponent": "sensor",
      "icon": "/drivers/braava/assets/tank_full.svg"
    }
  }
}